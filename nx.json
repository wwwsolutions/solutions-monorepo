      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.json": "*",
    "tslint.json": "*",
    "nx.json": "*"
  },
  "projects": {
    "admin-app-e2e": {
      "tags": ["type:e2e", "scope:admin"]
    },
    "admin-app": {
      "tags": ["type:app", "scope:admin"],
